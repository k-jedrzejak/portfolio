{"ast":null,"code":"import TextBlock from \"../components/TextBlock.vue\";\nimport GithubIcon from \"@/components/icon/GithubIcon.vue\";\nimport LinkedInIcon from \"@/components/icon/LinkedInIcon.vue\";\nimport SpineLine from \"@/components/SpineLine.vue\";\nimport { fetchData, removeBodyClass, addBodyClass } from \"@/utils\";\nimport { ref, onMounted } from \"vue\";\nexport default {\n  name: \"AboutView\",\n  components: {\n    TextBlock,\n    GithubIcon,\n    LinkedInIcon,\n    SpineLine\n  },\n\n  data() {\n    return {\n      data: {}\n    };\n  },\n\n  async mounted() {\n    this.data = await fetchData();\n  },\n\n  setup() {\n    const about = ref();\n    onMounted(() => {\n      if (about.value) {\n        removeBodyClass(\"home\");\n        addBodyClass(\"about\");\n      }\n    });\n    return {\n      about\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgHA,OAAOA,SAAP,MAAsB,6BAAtB;AACA,OAAOC,UAAP,MAAuB,kCAAvB;AACA,OAAOC,YAAP,MAAyB,oCAAzB;AACA,OAAOC,SAAP,MAAsB,4BAAtB;AACA,SAASC,SAAT,EAAoBC,eAApB,EAAqCC,YAArC,QAAyD,SAAzD;AACA,SAASC,GAAT,EAAcC,SAAd,QAA+B,KAA/B;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;EAEbC,UAAU,EAAE;IAAEV,SAAF;IAAaC,UAAb;IAAyBC,YAAzB;IAAuCC;EAAvC,CAFC;;EAGbQ,IAAI,GAAG;IACL,OAAO;MACLA,IAAI,EAAE;IADD,CAAP;EAGD,CAPY;;EAQb,MAAMC,OAAN,GAAgB;IACd,KAAKD,IAAL,GAAY,MAAMP,SAAS,EAA3B;EACD,CAVY;;EAWbS,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIP,GAAG,EAAjB;IACAC,SAAS,CAAC,MAAM;MACd,IAAIM,KAAK,CAACC,KAAV,EAAiB;QACfV,eAAe,CAAC,MAAD,CAAf;QACAC,YAAY,CAAC,OAAD,CAAZ;MACF;IACD,CALQ,CAAT;IAMA,OAAO;MAAEQ;IAAF,CAAP;EACD;;AApBY,CAAf","names":["TextBlock","GithubIcon","LinkedInIcon","SpineLine","fetchData","removeBodyClass","addBodyClass","ref","onMounted","name","components","data","mounted","setup","about","value"],"sourceRoot":"","sources":["/home/runner/work/portfolio/portfolio/src/views/AboutView.vue"],"sourcesContent":["<template>\n  <div id=\"about\" ref=\"about\" class=\"container\">\n    div\n    <div class=\"pic\"></div>\n    <div class=\"static-container\">\n      <h1\n        class=\"title\"\n        v-motion\n        :initial=\"{ opacity: 0, y: -80 }\"\n        :enter=\"{\n          opacity: 1,\n          y: 0,\n          transition: { delay: 300, duration: 800, ease: 'linear' },\n        }\"\n      >\n        about(<span class=\"params color\">karolina</span>)\n      </h1>\n\n      <TextBlock\n        v-motion\n        :initial=\"{ opacity: 0, x: -80 }\"\n        :enter=\"{\n          opacity: 1,\n          x: 0,\n          transition: { delay: 1000, duration: 800, ease: 'linear' },\n        }\"\n      >\n        <div class=\"first-fold\">\n          <ul class=\"about-contact\">\n            <li v-for=\"link in data.links\" :key=\"link.url\">\n              <a :href=\"link.url\" :title=\"link.title\" target=\"_blank\">\n                {{ link.label || null }}\n                <component v-if=\"link.icon\" :is=\"link.icon\" />\n              </a>\n            </li>\n            <li v-if=\"data.cv\">\n              Download my\n              <a\n                :href=\"`./${data.cv.file}`\"\n                :title=\"data.cv.title\"\n                target=\"_blank\"\n                class=\"cv\"\n              >\n                {{ data.cv.label }}\n              </a>\n            </li>\n          </ul>\n\n          <p class=\"color\">\n            <span v-for=\"(line, i) in data.description\" :key=\"i\">\n              {{ line }}<br />\n            </span>\n          </p>\n          <p class=\"gray\" v-if=\"data.experiences\">\n            <span v-for=\"(line, i) in data.subtitles\" :key=\"i\">\n              // {{ line }}<br />\n            </span>\n            // {{ data.experiences[0].position }} \n            <!-- TO DO: when company name is know -->\n            <!-- @ {{ data.experiences[0].company }} -->\n          </p>\n        </div>\n\n        <div class=\"about-grid\">\n          <h2>Skills</h2>\n          <div class=\"columns skills\">\n            <ul>\n              <li v-for=\"(skills, i) in data.skills\" :key=\"i\">\n                {{ skills.join(\", \") }}<br />\n              </li>\n            </ul>\n          </div>\n\n          <h2>Experience</h2>\n          <div class=\"columns experience\">\n            <ul>\n              <li v-for=\"(experience, i) in data.experiences\" :key=\"i\">\n                <strong class=\"color\">{{ experience.position }}</strong>\n                <br />\n                @ {{ experience.company }}<br />\n                {{ experience.time || null }}\n              </li>\n            </ul>\n          </div>\n\n          <h2>Languages</h2>\n          <div class=\"columns languages\">\n            <ul>\n              <li v-for=\"(langs, level) in data.languages\" :key=\"level\">\n                <span class=\"\">// {{ level }}</span\n                ><br />\n                <span v-for=\"[locale, label] in langs\" :key=\"locale\"\n                  ><span class=\"color\">{{ locale }}</span> {{ label }}<br\n                /></span>\n              </li>\n            </ul>\n          </div>\n\n          <h2>Also busy with</h2>\n          <div class=\"columns busy\">\n            <ul>\n              <li v-for=\"busy in data.busy\" :key=\"busy\">{{ busy }}</li>\n            </ul>\n          </div>\n        </div>\n      </TextBlock>\n    </div>\n    <SpineLine />\n  </div>\n</template>\n\n<script>\nimport TextBlock from \"../components/TextBlock.vue\";\nimport GithubIcon from \"@/components/icon/GithubIcon.vue\";\nimport LinkedInIcon from \"@/components/icon/LinkedInIcon.vue\";\nimport SpineLine from \"@/components/SpineLine.vue\";\nimport { fetchData, removeBodyClass, addBodyClass } from \"@/utils\";\nimport { ref, onMounted } from \"vue\";\n\nexport default {\n  name: \"AboutView\",\n  components: { TextBlock, GithubIcon, LinkedInIcon, SpineLine },\n  data() {\n    return {\n      data: {},\n    };\n  },\n  async mounted() {\n    this.data = await fetchData();\n  },\n  setup() {\n    const about = ref();\n    onMounted(() => {\n      if (about.value) {\n        removeBodyClass(\"home\");\n        addBodyClass(\"about\");\n      }\n    });\n    return { about };\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n$myImgPath: \"../assets/me9.webp\";\n\n.page-about {\n  .header-bg {\n    opacity: 0;\n    visibility: hidden;\n  }\n}\n\n#about {\n  .pic {\n    @include size(90vw);\n    @include fixed(10vh, $right: -67%);\n    transform: scaleX(-1);\n    pointer-events: none;\n    display: block;\n    z-index: 0;\n    aspect-ratio: 1;\n    @include bg-image($myImgPath, contain, right);\n    border-radius: 50%;\n\n    @media screen and (min-width: $screen-desktop) {\n      @include size(85vw);\n      @include fixed(70%, $left: 0);\n      background-position: left;\n      transform: translate(0, -50%);\n      border-radius: 0;\n      opacity: 1;\n    }\n  }\n  .static-container {\n    padding-top: 30vh;\n    @media screen and (min-width: $screen-desktop) {\n      .static-container {\n        padding-top: 30vh;\n      }\n    }\n  }\n  .about-contact {\n    @include flex();\n    list-style: none;\n    margin: 2rem 0;\n\n    li {\n      @include flex();\n      flex: 0 0 auto;\n      list-style: none;\n      margin: 0 1rem 0 0;\n      color: $color-white;\n    }\n\n    a {\n      @include flex();\n      flex: 0 0 auto;\n      text-decoration: none;\n      &.cv {\n        background: $color-primary;\n        @include size(auto, 2rem);\n        padding: 0 1rem;\n        border-radius: 3rem;\n        margin-left: 0.5rem;\n        line-height: 2rem;\n\n        &:hover {\n          background: $color-magenta-dark;\n        }\n      }\n\n      .ico {\n        @include size(1.5rem, 1.5rem);\n        flex: 0 0 auto;\n        overflow: visible;\n        fill: $color-primary;\n        stroke-width: 2px;\n        transition: all 400ms ease-out;\n      }\n\n      &:hover {\n        .ico {\n          fill: $color-gray;\n        }\n      }\n    }\n\n    @media screen and (max-width: $screen-mobile) {\n      flex-flow: row wrap;\n\n      li {\n        &:last-child {\n          flex: 0 0 100%;\n          margin: 1rem 0 0;\n        }\n      }\n    }\n  }\n\n  .about-grid {\n    @include size(65vw);\n    padding: 3rem 0 0 0;\n    margin-right: -25vw;\n\n    h2 {\n      font-size: 1.2rem;\n      margin-bottom: 1rem;\n    }\n\n    .columns {\n      margin-bottom: 2rem;\n    }\n\n    ul {\n      display: grid;\n      grid-template: 1fr / repeat(4, 1fr);\n      grid-gap: 2rem;\n      grid-row-gap: 1rem;\n    }\n\n    li {\n      list-style: none;\n      margin: 0;\n      font-size: 1rem;\n      line-height: 1.5em;\n      color: $color-gray;\n    }\n    .tools {\n      display: grid;\n      grid-template: 1fr / repeat(4, 1fr);\n      grid-gap: 2rem;\n      grid-row-gap: 1rem;\n\n      ul {\n        display: block;\n      }\n    }\n\n    @media screen and (max-width: $screen-desktop) {\n      @include size(calc(100vw - 4rem));\n      padding: 3rem 1rem 0 0;\n      margin: 0;\n    }\n\n    @media screen and (max-width: $screen-tablet) {\n      ul {\n        grid-template: 1fr / repeat(3, 1fr);\n      }\n\n      .tools {\n        grid-template: 1fr / repeat(3, 1fr);\n\n        ul {\n          display: block;\n        }\n      }\n    }\n\n    @media screen and (max-width: $screen-mobile) {\n      ul {\n        grid-template: 1fr / 1fr;\n        grid-gap: 1rem;\n      }\n\n      .tools {\n        grid-template: 1fr / 1fr;\n        grid-gap: 1rem;\n\n        ul {\n          display: block;\n        }\n      }\n    }\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}