{"ast":null,"code":"import TitleSection from \"../TitleSection.vue\";\nimport TitleFunction from \"../TitleFunction.vue\";\nimport TextBlock from \"../TextBlock.vue\";\nimport SceneSection from \"../SceneSection.vue\";\nimport AirbusLogo from \"../logotypes/Airbus/AirbusLogo.vue\";\nimport TheJupiter from \"../characters/jupiter/TheJupiter.vue\";\nimport AstroAir from \"../characters/airastro/AstroAir.vue\";\nexport default {\n  name: \"AirbusScene\",\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    AirbusLogo,\n    TheJupiter,\n    AstroAir\n  },\n  props: {\n    isPlaying: Boolean\n  }\n};","map":{"version":3,"mappings":"AAqEA,OAAOA,YAAP,MAAyB,qBAAzB;AACA,OAAOC,aAAP,MAA0B,sBAA1B;AACA,OAAOC,SAAP,MAAsB,kBAAtB;AACA,OAAOC,YAAP,MAAyB,qBAAzB;AACA,OAAOC,UAAP,MAAuB,oCAAvB;AACA,OAAOC,UAAP,MAAuB,sCAAvB;AACA,OAAOC,QAAP,MAAqB,qCAArB;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;EAEbC,UAAU,EAAE;IACVR,YADU;IAEVC,aAFU;IAGVC,SAHU;IAIVC,YAJU;IAKVC,UALU;IAMVC,UANU;IAOVC;EAPU,CAFC;EAWbG,KAAK,EAAE;IACLC,SAAS,EAAEC;EADN;AAXM,CAAf","names":["TitleSection","TitleFunction","TextBlock","SceneSection","AirbusLogo","TheJupiter","AstroAir","name","components","props","isPlaying","Boolean"],"sourceRoot":"","sources":["/home/runner/work/portfolio/portfolio/src/components/content/AirbusScene.vue"],"sourcesContent":["<template>\n  <div class=\"airbusScene\">\n    <TitleSection scene=\"airbusTitle\">\n      <TitleFunction\n        selectorId=\"airbus\"\n        subtitle=\"// from: August 2018\"\n        subtitle2=\"// to: March 2019\"\n      ></TitleFunction>\n    </TitleSection>\n    <SceneSection id=\"airbus1\">\n      <TextBlock>\n        <p>\n          Before I charted the digital realms as a Frontend Developer, I\n          ventured into space\n        </p>\n        <p class=\"big\">\n          as a\n          <strong class=\"color\">Process Engineer</strong><br />\n        </p>\n        <p>in the space department</p>\n        <p class=\"big\">\n          @\n          <a\n            href=\"https://www.airbus.com/en/our-worldwide-presence/airbus-in-europe/airbus-in-poland\"\n            target=\"_blank\"\n            title=\"Visit Airbus website\"\n            >Airbus</a\n          >.\n        </p>\n        <p>\n          - Pioneer of a sustainable aerospace industry for a safe and united\n          world.\n        </p>\n      </TextBlock>\n      <template #animeContainer>\n        <AirbusLogo />\n        <TheJupiter :isPlaying=\"isPlaying\" />\n        <AstroAir :isPlaying=\"isPlaying\" />\n      </template>\n    </SceneSection>\n\n    <SceneSection id=\"airbus2\">\n      <TextBlock>\n        <p>My mission echoed among the stars</p>\n        <p class=\"big\">\n          as I worked on the\n          <strong class=\"color\">ESA's JUICE probe</strong> project, destined to\n          unveil the secrets of <strong class=\"color\">Jupiter's moons</strong>.\n        </p>\n      </TextBlock>\n    </SceneSection>\n\n    <SceneSection id=\"airbus3\">\n      <TextBlock>\n        <p>By selecting components for its</p>\n        <p class=\"big\">\n          <strong class=\"color\">electrical harnesses</strong> and\n          <strong class=\"color\">developing programs</strong> to test them using\n          advanced machinery,\n        </p>\n        <p>\n          I bridged the heavens and the earth through precision and innovation.\n        </p>\n      </TextBlock>\n    </SceneSection>\n  </div>\n</template>\n\n<script>\nimport TitleSection from \"../TitleSection.vue\";\nimport TitleFunction from \"../TitleFunction.vue\";\nimport TextBlock from \"../TextBlock.vue\";\nimport SceneSection from \"../SceneSection.vue\";\nimport AirbusLogo from \"../logotypes/Airbus/AirbusLogo.vue\";\nimport TheJupiter from \"../characters/jupiter/TheJupiter.vue\";\nimport AstroAir from \"../characters/airastro/AstroAir.vue\";\n\nexport default {\n  name: \"AirbusScene\",\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    AirbusLogo,\n    TheJupiter,\n    AstroAir,\n  },\n  props: {\n    isPlaying: Boolean,\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#airbusLogo {\n  @include absolute(5vh, null, null, 50%);\n  transform: translateX(-50%);\n  filter: brightness(2);\n  z-index: 4;\n\n  svg {\n    @include size(35vh, 35vh);\n  }\n}\n\n.jupiter {\n  @include size(35vh, 35vh);\n  @include absolute(17vh, -2vh, null, null);\n  filter: brightness(0.9);\n}\n\n.probe {\n  @include size(45vh, 45vh);\n  @include absolute(12vh, -7vh, null, null);\n\n  #theProbe {\n    @include size(10vh, 10vh);\n    filter: brightness(0.9);\n  }\n}\n\n#astroAir {\n  @include absolute(null, null, 5vh, 0);\n  svg {\n    @include size(14vh, 17vh);\n    filter: brightness(0.9);\n  }\n}\n\n// Tablet\n@media screen and (min-width: $screen-tablet) {\n  #airbusLogo {\n    @include absolute(15vh, null, null, 22vh);\n    svg {\n      @include size(30vh, 30vh);\n    }\n  }\n  .jupiter {\n    @include size(35vh, 35vh);\n    @include absolute(17vh, 8vh, null, null);\n  }\n\n  .probe {\n    @include size(45vh, 45vh);\n    @include absolute(12vh, 3vh, null, null);\n\n    #theProbe {\n      @include size(10vh, 10vh);\n    }\n  }\n\n  #astroAir {\n    @include absolute(null, null, 5vh, 0);\n\n    svg {\n      @include size(21vh, 24vh);\n    }\n  }\n}\n\n// Desktop\n@media screen and (min-width: $screen-desktop) {\n  #airbusLogo {\n    @include absolute(35vh, null, null, 22vh);\n    svg {\n      @include size(40vh, 20vh);\n    }\n  }\n  .jupiter {\n    @include size(50vh, 50vh);\n    @include absolute(10vh, 2vh, null, null);\n    filter: brightness(1);\n  }\n\n  .probe {\n    @include size(65vh, 65vh);\n    @include absolute(3vh, -5vh, null, null);\n\n    #theProbe {\n      @include size(15vh, 15vh);\n      filter: brightness(1);\n    }\n  }\n\n  #astroAir {\n    @include absolute(null, null, 5vh, 0);\n\n    svg {\n      @include size(32vh, 35vh);\n      filter: brightness(1);\n    }\n  }\n}\n</style>\n"]},"metadat