{"ast":null,"code":"import TitleSection from \"../TitleSection.vue\";\nimport TitleFunction from \"../TitleFunction.vue\";\nimport TextBlock from \"../TextBlock.vue\";\nimport SceneSection from \"../SceneSection.vue\";\nimport EuroLogo from \"../logotypes/Euro/EuroLogo.vue\";\nimport ThePlanet from \"../characters/planet/ThePlanet.vue\";\nexport default {\n  name: \"EuroScene\",\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    EuroLogo,\n    ThePlanet\n  },\n  props: {\n    isPlaying: Boolean\n  }\n};","map":{"version":3,"mappings":"AAoEA,OAAOA,YAAP,MAAyB,qBAAzB;AACA,OAAOC,aAAP,MAA0B,sBAA1B;AACA,OAAOC,SAAP,MAAsB,kBAAtB;AACA,OAAOC,YAAP,MAAyB,qBAAzB;AACA,OAAOC,QAAP,MAAqB,gCAArB;AACA,OAAOC,SAAP,MAAsB,oCAAtB;AAEA,eAAe;EACbC,IAAI,EAAE,WADO;EAEbC,UAAU,EAAE;IACVP,YADU;IAEVC,aAFU;IAGVC,SAHU;IAIVC,YAJU;IAKVC,QALU;IAMVC;EANU,CAFC;EAUbG,KAAK,EAAE;IACLC,SAAS,EAAEC;EADN;AAVM,CAAf","names":["TitleSection","TitleFunction","TextBlock","SceneSection","EuroLogo","ThePlanet","name","components","props","isPlaying","Boolean"],"sourceRoot":"","sources":["/home/runner/work/portfolio/portfolio/src/components/content/EuroScene.vue"],"sourcesContent":["<template>\n  <div class=\"euroScene\">\n    <TitleSection scene=\"euroTitle\">\n      <TitleFunction\n        selectorId=\"euroNet\"\n        subtitle=\"// from: August 2019\"\n        subtitle2=\"// to: February 2021\"\n      ></TitleFunction>\n    </TitleSection>\n\n    <SceneSection id=\"euro1\">\n      <TextBlock>\n        <p>From August 2019 to February 2021</p>\n        <p class=\"big\">\n          I was a\n          <strong class=\"color\">Junior Frontend Developer</strong> <br />\n          @ Euro-Net.\n        </p>\n        <p>\n          Euro-Net owns two of the largest eCommerce with electronics in Poland\n          -\n          <a\n            href=\"https://www.euro.com.pl/\"\n            target=\"_blank\"\n            title=\"Visit RTV Euro AGD website\"\n          >\n            RTV Euro AGD</a\n          >\n          and\n          <a\n            href=\"https://www.oleole.pl/\"\n            target=\"_blank\"\n            title=\"Visit OleOle.pl website\"\n            >OleOle.pl</a\n          >\n        </p>\n      </TextBlock>\n      <template #animeContainer>\n        <ThePlanet :isPlaying=\"isPlaying\" />\n        <EuroLogo />\n      </template>\n    </SceneSection>\n\n    <SceneSection id=\"euro2\">\n      <TextBlock>\n        <p>My responsibilites were</p>\n        <p class=\"big\">\n          <strong class=\"color\"\n            >creating and implementing landing pages of products and\n            promotions</strong\n          >\n        </p>\n      </TextBlock>\n    </SceneSection>\n\n    <SceneSection id=\"euro3\">\n      <TextBlock>\n        <p>As there were many products and promotions</p>\n        <p class=\"big\">\n          <strong class=\"color\">the code had to be light and optimized</strong>\n          not to burden the main pages.\n        </p>\n      </TextBlock>\n    </SceneSection>\n  </div>\n</template>\n\n<script>\nimport TitleSection from \"../TitleSection.vue\";\nimport TitleFunction from \"../TitleFunction.vue\";\nimport TextBlock from \"../TextBlock.vue\";\nimport SceneSection from \"../SceneSection.vue\";\nimport EuroLogo from \"../logotypes/Euro/EuroLogo.vue\";\nimport ThePlanet from \"../characters/planet/ThePlanet.vue\";\n\nexport default {\n  name: \"EuroScene\",\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    EuroLogo,\n    ThePlanet,\n  },\n  props: {\n    isPlaying: Boolean,\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#euroLogo {\n  @include absolute(30%, null, null, 50%);\n  transform: translate(-50%, -50%);\n  @include size(70vw);\n  opacity: 0.6;\n}\n\n#toaster {\n  @include relative(null, null, -12rem, -1rem);\n  @include size(200px);\n  z-index: 9;\n}\n\n.planet {\n  @include size(60rem, 60rem);\n  @include relative(-2rem, null, null, -25rem);\n}\n\n// Tablet\n@media screen and (min-width: $screen-tablet) {\n  #euroLogo {\n    @include size(50vw);\n  }\n\n  .planet {\n    @include size(80rem, 80rem);\n  }\n\n  #toaster {\n    @include size(350px);\n  }\n}\n\n// Desktop\n@media screen and (min-width: $screen-desktop) {\n  #euroLogo {\n    @include absolute(20%, null, null, null);\n    @include size(40vw);\n  }\n\n  #toaster {\n    @include relative(null, null, -8rem, -2rem);\n    @include size(450px);\n    z-index: 9;\n  }\n\n  .planet {\n    @include size(100rem, 100rem);\n    @include relative(-3rem, null, null, -35rem);\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}