{"ast":null,"code":"import TitleSection from \"../TitleSection.vue\";\nimport TitleFunction from \"../TitleFunction.vue\";\nimport TextBlock from \"../TextBlock.vue\";\nimport SceneSection from \"../SceneSection.vue\";\nimport UfoRhino from \"../characters/rhino/UfoRhino.vue\";\nimport AdrinoLogo from \"../logotypes/Adrino/AdrinoLogo.vue\";\nexport default {\n  name: \"AdrinoScene\",\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    UfoRhino,\n    AdrinoLogo\n  },\n  props: {\n    isPlaying: Boolean\n  }\n};","map":{"version":3,"mappings":"AA0EA,OAAOA,YAAP,MAAyB,qBAAzB;AACA,OAAOC,aAAP,MAA0B,sBAA1B;AACA,OAAOC,SAAP,MAAsB,kBAAtB;AACA,OAAOC,YAAP,MAAyB,qBAAzB;AACA,OAAOC,QAAP,MAAqB,kCAArB;AACA,OAAOC,UAAP,MAAuB,oCAAvB;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;EAEbC,UAAU,EAAE;IACVP,YADU;IAEVC,aAFU;IAGVC,SAHU;IAIVC,YAJU;IAKVC,QALU;IAMVC;EANU,CAFC;EAUbG,KAAK,EAAE;IACLC,SAAS,EAAEC;EADN;AAVM,CAAf","names":["TitleSection","TitleFunction","TextBlock","SceneSection","UfoRhino","AdrinoLogo","name","components","props","isPlaying","Boolean"],"sourceRoot":"","sources":["/home/runner/work/portfolio/portfolio/src/components/content/AdrinoScene.vue"],"sourcesContent":["<template>\n  <div class=\"adrinoScene\">\n    <TitleSection scene=\"adrinoTitle\">\n      <TitleFunction\n        selectorId=\"adrino\"\n        subtitle=\"// from: December 2022\"\n        subtitle2=\"// to: September 2023\"\n      ></TitleFunction>\n    </TitleSection>\n\n    <SceneSection id=\"adrino1\">\n      <TextBlock>\n        <p>\n          From December 2022's start to September 2023's end, my journey\n          unfolded\n        </p>\n        <p class=\"big\">\n          as a\n          <strong class=\"color\">JavaScript Developer</strong><br />\n          @\n          <a\n            href=\"https://www.adrino.pl/\"\n            target=\"_blank\"\n            title=\"Visit Adrino website\"\n            >Adrino Mobile</a\n          >.\n        </p>\n        <p>\n          Adrino's in-house ecosystem consists of original rich media creations,\n          classic display adverts, native adverts.\n        </p>\n      </TextBlock>\n      <template #animeContainer>\n        <UfoRhino :isPlaying=\"isPlaying\" />\n        <AdrinoLogo />\n      </template>\n    </SceneSection>\n\n    <SceneSection id=\"adrino2\">\n      <TextBlock>\n        <p>I was responsible for</p>\n        <p class=\"big\">\n          breathing life into graphic designs using\n          <strong class=\"color\">JavaScript</strong> and\n          <strong class=\"color\">animations</strong>.\n        </p>\n      </TextBlock>\n    </SceneSection>\n\n    <SceneSection id=\"adrino3\">\n      <TextBlock>\n        <p>I dove deep into the realm of</p>\n        <p class=\"big\">\n          crafting <strong class=\"color\">interactive ads</strong> that weren't\n          just adverts, but <strong class=\"color\">mini-games</strong> that\n          players wanted to immerse in.\n        </p>\n      </TextBlock>\n    </SceneSection>\n\n    <SceneSection id=\"adrino4\">\n      <TextBlock>\n        <p>In the labyrinth of the digital world,</p>\n        <p class=\"big\">\n          I sculpted a control panel, becoming the bridge between\n          <strong class=\"color\">publishers</strong> and their dazzling\n          <strong class=\"color\">advertisements</strong>.\n        </p>\n      </TextBlock>\n    </SceneSection>\n  </div>\n</template>\n\n<script>\nimport TitleSection from \"../TitleSection.vue\";\nimport TitleFunction from \"../TitleFunction.vue\";\nimport TextBlock from \"../TextBlock.vue\";\nimport SceneSection from \"../SceneSection.vue\";\nimport UfoRhino from \"../characters/rhino/UfoRhino.vue\";\nimport AdrinoLogo from \"../logotypes/Adrino/AdrinoLogo.vue\";\n\nexport default {\n  name: \"AdrinoScene\",\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    UfoRhino,\n    AdrinoLogo,\n  },\n  props: {\n    isPlaying: Boolean,\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#adrinoLogo {\n  @include absolute(15vh, 4vh, null, null);\n  z-index: 10;\n}\n\n#adrinoLogoImage {\n  @include size(25vh);\n}\n\n#ufoRhino {\n  @include absolute(15vh, 0);\n  @include size(40vh, 100vh);\n  z-index: 3;\n}\n\n// Tablet\n@media screen and (min-width: $screen-tablet) {\n  #adrinoLogo {\n    @include absolute(12vh, 2vh, null, null);\n  }\n\n  #ufoRhino {\n    @include absolute(12vh, 0);\n    @include size(40vh);\n  }\n}\n\n// Desktop\n@media screen and (min-width: $screen-desktop) {\n  #adrinoLogo {\n    @include absolute(30vh, null, null, 10vh);\n  }\n\n  #ufoRhino {\n    @include absolute(18vh, 0);\n    @include size(auto);\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}