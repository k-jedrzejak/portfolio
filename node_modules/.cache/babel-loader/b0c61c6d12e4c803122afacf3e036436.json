{"ast":null,"code":"import TitleSection from \"../TitleSection.vue\";\nimport TitleFunction from \"../TitleFunction.vue\";\nimport TextBlock from \"../TextBlock.vue\";\nimport SceneSection from \"../SceneSection.vue\";\nimport PracaLogo from \"../logotypes/Praca/PracaLogo.vue\";\nimport AstroWork from \"../characters/work/AstroWork.vue\";\nexport default {\n  name: \"AuroraScene\",\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    PracaLogo,\n    AstroWork\n  },\n  props: {\n    isPlaying: Boolean\n  }\n};","map":{"version":3,"mappings":"AA8DA,OAAOA,YAAP,MAAyB,qBAAzB;AACA,OAAOC,aAAP,MAA0B,sBAA1B;AACA,OAAOC,SAAP,MAAsB,kBAAtB;AACA,OAAOC,YAAP,MAAyB,qBAAzB;AACA,OAAOC,SAAP,MAAsB,kCAAtB;AACA,OAAOC,SAAP,MAAsB,kCAAtB;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;EAEbC,UAAU,EAAE;IACVP,YADU;IAEVC,aAFU;IAGVC,SAHU;IAIVC,YAJU;IAKVC,SALU;IAMVC;EANU,CAFC;EAUbG,KAAK,EAAE;IACLC,SAAS,EAAEC;EADN;AAVM,CAAf","names":["TitleSection","TitleFunction","TextBlock","SceneSection","PracaLogo","AstroWork","name","components","props","isPlaying","Boolean"],"sourceRoot":"","sources":["/home/runner/work/portfolio/portfolio/src/components/content/PracaScene.vue"],"sourcesContent":["<template>\n  <div class=\"pracaScene\">\n    <TitleSection scene=\"pracaTitle\">\n      <TitleFunction\n        selectorId=\"pracaPl\"\n        subtitle=\"// from: October 2018\"\n        subtitle2=\"// to: March 2019\"\n      ></TitleFunction>\n    </TitleSection>\n\n    <SceneSection id=\"praca1\">\n      <TextBlock>\n        <p>I put on the cloak of a</p>\n\n        <p class=\"big\">\n          <strong class=\"color\"> Junior Frontend Developer</strong>\n        </p>\n        <p>and cast my first spell in the realm of professional code</p>\n        <p class=\"big\">\n          @\n          <a\n            href=\"https://www.praca.pl/\"\n            target=\"_blank\"\n            title=\"Visit Praca.pl website\"\n            >Praca.pl</a\n          >.\n        </p>\n        <p>Praca.pl is one of the largest service with job offers in Poland</p>\n      </TextBlock>\n      <template #animeContainer>\n        <AstroWork :isPlaying=\"isPlaying\" />\n        <PracaLogo />\n      </template>\n    </SceneSection>\n\n    <SceneSection id=\"praca2\">\n      <TextBlock>\n        <p class=\"big\">\n          <strong class=\"color\"\n            >Creating landing pages about specific positions</strong\n          >.\n        </p>\n        <p>\n          Landing pages have contained subpages and implemented job offers from\n          the home page.\n        </p>\n      </TextBlock>\n    </SceneSection>\n\n    <SceneSection id=\"praca3\">\n      <TextBlock>\n        <p class=\"big\"><strong class=\"color\">SEO implementation</strong>.</p>\n        <p>\n          The most important thing for the pages was to have the highest\n          possible position in Google search results.\n        </p>\n      </TextBlock>\n    </SceneSection>\n  </div>\n</template>\n\n<script>\nimport TitleSection from \"../TitleSection.vue\";\nimport TitleFunction from \"../TitleFunction.vue\";\nimport TextBlock from \"../TextBlock.vue\";\nimport SceneSection from \"../SceneSection.vue\";\nimport PracaLogo from \"../logotypes/Praca/PracaLogo.vue\";\nimport AstroWork from \"../characters/work/AstroWork.vue\";\n\nexport default {\n  name: \"AuroraScene\",\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    PracaLogo,\n    AstroWork,\n  },\n  props: {\n    isPlaying: Boolean,\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#pracaLogo {\n  @include absolute(15vh, null, null, 3vh);\n  @include size(30vh);\n  z-index: 10;\n}\n\n#astroWork {\n  @include absolute(null, 2vh, 8vh, null);\n  @include size(35vh, auto);\n  filter: brightness(0.9);\n}\n\n// Tablet\n@media screen and (min-width: $screen-tablet) {\n  #pracaLogo {\n    @include size(40vh);\n    @include absolute(20vh, null, null, 10vh);\n    filter: brightness(1.2);\n  }\n\n  #astroWork {\n    @include size(40vh, auto);\n  }\n}\n\n// Desktop\n@media screen and (min-width: $screen-desktop) {\n  #pracaLogo {\n    @include size(45vh);\n    @include absolute(25vh, null, null, 50vh);\n    filter: brightness(1.6);\n  }\n  #astroWork {\n    @include size(65vh);\n    @include absolute(null, 8vh, 10vh, null);\n    filter: brightness(1);\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}