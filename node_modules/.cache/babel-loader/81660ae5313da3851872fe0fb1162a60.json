{"ast":null,"code":"import TitleSection from \"../TitleSection.vue\";\nimport TitleFunction from \"../TitleFunction.vue\";\nimport TextBlock from \"../TextBlock.vue\";\nimport SceneSection from \"../SceneSection.vue\";\nimport AcLogo from \"../logotypes/AC/AcLogo.vue\";\nimport AstroMagento from \"../characters/magento/AstroMagento.vue\";\nexport default {\n  name: \"AuroraScene\",\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    AcLogo,\n    AstroMagento\n  },\n  props: {\n    isPlaying: Boolean\n  }\n};","map":{"version":3,"mappings":"AAgEA,OAAOA,YAAP,MAAyB,qBAAzB;AACA,OAAOC,aAAP,MAA0B,sBAA1B;AACA,OAAOC,SAAP,MAAsB,kBAAtB;AACA,OAAOC,YAAP,MAAyB,qBAAzB;AACA,OAAOC,MAAP,MAAmB,4BAAnB;AACA,OAAOC,YAAP,MAAyB,wCAAzB;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;EAEbC,UAAU,EAAE;IACVP,YADU;IAEVC,aAFU;IAGVC,SAHU;IAIVC,YAJU;IAKVC,MALU;IAMVC;EANU,CAFC;EAUbG,KAAK,EAAE;IACLC,SAAS,EAAEC;EADN;AAVM,CAAf","names":["TitleSection","TitleFunction","TextBlock","SceneSection","AcLogo","AstroMagento","name","components","props","isPlaying","Boolean"],"sourceRoot":"","sources":["/home/runner/work/portfolio/portfolio/src/components/content/AuroraScene.vue"],"sourcesContent":["<template>\n  <div class=\"auroraScene\">\n    <TitleSection scene=\"auroraTitle\">\n      <TitleFunction\n        selectorId=\"auroraCreation\"\n        subtitle=\"// from: February 2021\"\n        subtitle2=\"// to: December 2022\"\n      ></TitleFunction>\n    </TitleSection>\n\n    <SceneSection id=\"aurora1\">\n      <TextBlock>\n        <p>In this quest in the vast expanse of e-commerce</p>\n        <p class=\"big\">\n          I was a\n          <strong class=\"color\">Frontend Developer</strong><br />\n          @\n          <a\n            href=\"https://www.auroracreation.pl/\"\n            target=\"_blank\"\n            title=\"Visit AC website\"\n            >Aurora Creation</a\n          >.\n        </p>\n        <p>\n          Software House implementing, simplifying, customizing and developing\n          Magento for top Polish eCommerce brands.\n        </p>\n      </TextBlock>\n      <template #animeContainer>\n        <AcLogo />\n        <AstroMagento :isPlaying=\"isPlaying\" />\n      </template>\n    </SceneSection>\n\n    <SceneSection id=\"aurora2\">\n      <TextBlock>\n        <p>During that time</p>\n        <p class=\"big\">\n          I delivered\n          <strong class=\"color\"\n            >new functionalities and adjusted the appearance of online\n            stores</strong\n          >\n        </p>\n        <p>based on graphic designs.</p>\n      </TextBlock>\n    </SceneSection>\n\n    <SceneSection id=\"aurora3\">\n      <TextBlock>\n        <p>Taking care of</p>\n        <p class=\"big\">\n          <strong class=\"color\"\n            >code quality, optimization and improvement of store\n            efficiency</strong\n          >.\n        </p>\n      </TextBlock>\n    </SceneSection>\n  </div>\n</template>\n\n<script>\nimport TitleSection from \"../TitleSection.vue\";\nimport TitleFunction from \"../TitleFunction.vue\";\nimport TextBlock from \"../TextBlock.vue\";\nimport SceneSection from \"../SceneSection.vue\";\nimport AcLogo from \"../logotypes/AC/AcLogo.vue\";\nimport AstroMagento from \"../characters/magento/AstroMagento.vue\";\n\nexport default {\n  name: \"AuroraScene\",\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    AcLogo,\n    AstroMagento,\n  },\n  props: {\n    isPlaying: Boolean,\n  },\n};\n</script>\n\n<style lang=\"scss\">\n#acLogo {\n  @include size(45vh, 45vh);\n  @include absolute(10vh, null, null, -9vh);\n  z-index: 10;\n}\n\n#astroMagento {\n  @include absolute(22vh, 0);\n  @include size(32vh, 100vh);\n  z-index: 3;\n  filter: brightness(0.9);\n}\n\n// Tablet\n@media screen and (min-width: $screen-tablet) {\n  #acLogo {\n    @include size(50vh, 50vh);\n    @include absolute(15vh, null, null, -10vh);\n  }\n\n  #astroMagento {\n    @include size(45vh);\n    @include absolute(15vh, 0);\n  }\n}\n\n// Desktop\n@media screen and (min-width: $screen-desktop) {\n  #acLogo {\n    @include absolute(20vh, null, null, -2vh);\n    @include size(60vh, 85vh);\n  }\n\n  #astroMagento {\n    @include size(50vh, 100vh);\n    @include absolute(10vh, 0 null, null);\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}