{"ast":null,"code":"import TitleSection from '../TitleSection.vue';\nimport TitleFunction from '../TitleFunction.vue';\nimport TextBlock from '../TextBlock.vue';\nimport SceneSection from '../SceneSection.vue';\nimport PracaLogo from '../logotypes/Praca/PracaLogo.vue';\nimport AstroWork from '../characters/work/AstroWork.vue';\nexport default {\n  name: 'AuroraScene',\n  components: {\n    TitleSection,\n    TitleFunction,\n    TextBlock,\n    SceneSection,\n    PracaLogo,\n    AstroWork\n  },\n  props: {\n    isPlaying: Boolean\n  }\n};","map":{"version":3,"mappings":"AA8CE,OAAOA,YAAP,MAAyB,qBAAzB;AACA,OAAOC,aAAP,MAA0B,sBAA1B;AACA,OAAOC,SAAP,MAAsB,kBAAtB;AACA,OAAOC,YAAP,MAAyB,qBAAzB;AACA,OAAOC,SAAP,MAAsB,kCAAtB;AACA,OAAOC,SAAP,MAAsB,kCAAtB;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;EAEbC,UAAU,EAAE;IACVP,YADU;IAEVC,aAFU;IAGVC,SAHU;IAIVC,YAJU;IAKVC,SALU;IAMVC;EANU,CAFC;EAUbG,KAAK,EAAE;IACHC,SAAS,EAAEC;EADR;AAVM,CAAf","names":["TitleSection","TitleFunction","TextBlock","SceneSection","PracaLogo","AstroWork","name","components","props","isPlaying","Boolean"],"sourceRoot":"","sources":["/home/runner/work/portfolio/portfolio/src/components/content/PracaScene.vue"],"sourcesContent":["<template>\n    <div class=\"pracaScene\">\n      <TitleSection scene=\"pracaTitle\">\n        <TitleFunction selectorId=\"pracaPl\" subtitle=\"// from: March 2019\" subtitle2=\"// to: August 2019\"></TitleFunction>\n      </TitleSection>\n  \n      <SceneSection id=\"praca1\">\n        <TextBlock>\n        <p> March 2019 to August 2019</p>\n        <p class=\"big\">\n          <strong class=\"color\"> Junior Frontend Developer</strong><br/>\n          @\n          <a\n            href=\"https://www.praca.pl/\"\n            target=\"_blank\"\n            title=\"Visit Praca.pl website\"\n          >Praca.pl</a\n          >.\n        </p>\n        <p>\n          Praca.pl is one of the largest service with job offers in Poland </p>\n      </TextBlock>\n      <template #animeContainer>\n        <AstroWork :isPlaying=\"isPlaying\"/>\n        <PracaLogo />\n      </template>\n      </SceneSection>\n  \n      <SceneSection id=\"praca2\">\n        <TextBlock>\n          <p class=\"big\"><strong class=\"color\">Creating landing pages about specific positions</strong>.</p>\n          <p> Landing pages have contained subpages and implemented job offers from the home page.</p>\n        </TextBlock>\n      </SceneSection>\n  \n      <SceneSection id=\"praca3\">\n        <TextBlock>\n         \n          <p class=\"big\"><strong class=\"color\">SEO implementation</strong>.</p>\n          <p>The most important thing for the pages was to have the highest possible position in Google search results.</p> \n        </TextBlock>\n      </SceneSection>\n    </div>\n</template>\n  \n<script>\n  import TitleSection from '../TitleSection.vue'\n  import TitleFunction from '../TitleFunction.vue'\n  import TextBlock from '../TextBlock.vue'\n  import SceneSection from '../SceneSection.vue'\n  import PracaLogo from '../logotypes/Praca/PracaLogo.vue'\n  import AstroWork from '../characters/work/AstroWork.vue'\n\n  export default {\n    name: 'AuroraScene',\n    components: {\n      TitleSection,\n      TitleFunction,\n      TextBlock,\n      SceneSection,\n      PracaLogo,\n      AstroWork\n    },\n    props: {\n        isPlaying: Boolean,\n    },\n  }\n  </script>\n  \n\n<style lang=\"scss\">\n  #pracaLogo {\n    @include absolute(15%, null, null, 10%);\n    @include size(70vw);\n    z-index: 10;\n  }\n\n  #astroWork {\n    @include absolute(null, 3rem, 5rem, null);\n    @include size(55vw, auto);\n  }\n\n\n  // Tablet\n  @media screen and (min-width: $screen-tablet) {\n    #pracaLogo {\n      @include size(40vw);\n      @include absolute(20%, null, null, 10%);\n    }\n\n    #astroWork {\n      @include size(30vw, auto);\n    }\n  }\n\n  // Desktop\n  @media screen and (min-width: $screen-desktop) {\n    #pracaLogo {\n      @include size(30vw);\n      @include absolute(20%, null, null, 25%);\n    }\n    #astroWork {\n      @include absolute(null, 8rem, 3rem, null);\n    }\n  }\n    \n  </style>"]},"metadata":{},"sourceType":"module"}